{
  "routes": [
    {
      "src": "/api/*",
      "dest": "api/index.js",
      "headers": {
        "Cache-Control": "no-cache",
        "Access-Control-Allow-Origin": "*"
      }
    },
    {
      "src": "/",
      "dest": "index.html",
      "headers": {
        "Cache-Control": "public, max-age=0"
      }
    }
  ]
}
